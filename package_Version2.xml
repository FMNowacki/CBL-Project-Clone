<?xml version="1.0"?>
<package format="3">
  <name>my_ros2_package</name>
  <version>0.0.1</version>
  <description>Custom ROS2 package for TurtleBot3 frontier exploration and SLAM visualization.</description>
  <maintainer email="f.m.nowacki@student.tue.nl">Your Name</maintainer>
  <license>Apache License 2.0</license>

  <!-- Dependencies -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <exec_depend>rclpy</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>nav_msgs</exec_depend>
  <exec_depend>turtlebot3_msgs</exec_depend>
  <exec_depend>turtlebot3_navigation2</exec_depend>
  <exec_depend>slam_toolbox</exec_depend>
  <exec_depend>rviz2</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
